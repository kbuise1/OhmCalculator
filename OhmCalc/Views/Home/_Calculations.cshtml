@model OhmCalc.Models.CalculationModel
@{
    @Scripts.Render("~/Scripts/jquery-1.10.2.js")
    @Scripts.Render("~/Scripts/jquery.unobtrusive-ajax.min.js")
}




<div class="row form-group form-inline">
    <div class="col-md-3">
        @Html.LabelFor(m => m.OhmValue)
        @Html.TextBoxFor(m => m.OhmValue)
    </div>
    <div class="col-md-3">
        @Html.LabelFor(m => m.Tolerance)
        @Html.TextBoxFor(m => m.Tolerance)
    </div>
    <div class="col-md-3">
        @Html.LabelFor(m => m.MinOhm)
        @Html.TextBoxFor(m => m.MinOhm)
    </div>
    <div class="col-md-3">
        @Html.LabelFor(m => m.MaxOhm)
        @Html.TextBoxFor(m => m.MaxOhm)
    </div>
    </div>


